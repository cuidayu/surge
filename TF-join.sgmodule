!name=TF-join
!desc=自动加入TF

[Script]
TF =type=cron,cronexp=0/5 * * * * *,script-path=https://raw.githubusercontent.com/Yuheng0101/X/main/Tasks/AutoJoinTF.js,timeout=30,enable=true
AutoJoinTF = type=http-request,pattern=^https:\/\/testflight\.apple\.com\/(v3\/accounts\/.*[^\/accept]|join\/[A-Za-z0-9]+)$,requires-body=0,max-size=0,script-path=https://raw.githubusercontent.com/Yuheng0101/X/main/Tasks/AutoJoinTF.js

[MITM]
hostname = %APPEND% testflight.apple.com
